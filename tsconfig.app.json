{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./env.d.ts",
    "app/.vitepress/src/**/*",
    "app/.vitepress/src/**/*.vue",
    "app/.vitepress/config.ts",
    "app/.vitepress/theme/index.ts"
  ],
  "exclude": ["app/.vitepress/src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["app/.vitepress/src/*"]
    }
  }
}
